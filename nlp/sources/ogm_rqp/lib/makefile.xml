<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>lib</type>
  <project_name>ogm_rqp</project_name>
  <sources>
    <file>irqp.c</file>
    <file>rqpapi.c</file>
    <file>rqpba.c</file>
    <file>rqpbx.c</file>
    <file>rqpclean.c</file>
    <file>rqpequiv.c</file>
    <file>rqpfpar.c</file>
    <file>rqpfunc.c</file>
    <file>rqplog.c</file>
    <file>rqpmem.c</file>
    <file>rqpminus.c</file>
    <file>rqpmisc.c</file>
    <file>rqpnode.c</file>
    <file>rqpnormd.c</file>
    <file>rqpparse.c</file>
    <file>rqpprety.c</file>
    <file>rqppretyxml.c</file>
    <file>rqpprior.c</file>
    <file>rqprepar.c</file>
    <file>rqpstruc.c</file>
    <file>rqpsubtree.c</file>
  </sources>
  <platforms>
    <platform name="win">
      <option name="NAME">ogm_rqp</option>
      <option name="ADD_INC"/>
      <option name="ADD_LIBR">ogm_aut.lib ogm_uni.lib ogm_msg.lib ogm_xml.lib ogm_thr.lib ogm_gen.lib</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
    <platform name="linux">
      <option name="NAME">ogrqp</option>
      <option name="ADD_CFLAGS"/>
      <option name="ADD_LIBR">-logaut -loggen -logmsg -logthr -loguni -logxml</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
  </platforms>
</makefile>
