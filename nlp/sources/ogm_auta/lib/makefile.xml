<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>lib</type>
  <project_name>ogm_auta</project_name>
  <sources>
    <file>charset.c</file>
    <file>delword.c</file>
    <file>entity.c</file>
    <file>quaranti.c</file>
  </sources>
  <platforms>
    <platform name="win">
      <option name="NAME">ogm_auta</option>
      <option name="ADD_INC"/>
      <option name="ADD_LIBR">ogm_aut.lib ogm_gen.lib</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
    <platform name="linux">
      <option name="NAME">ogauta</option>
      <option name="ADD_CFLAGS"/>
      <option name="ADD_LIBR">-logaut -loggen</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
  </platforms>
</makefile>
