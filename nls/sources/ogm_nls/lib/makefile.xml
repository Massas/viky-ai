<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>lib</type>
  <project_name>ogm_nls</project_name>
  <sources>
    <file>inls.c</file>
    <file>nlsconf.c</file>
    <file>nlsimport.c</file>
    <file>nlsonem.c</file>
    <file>nlsrun.c</file>
    <file>nlslog.c</file>
    <file>nlslt.c</file>
    <file>nlserror.c</file>
    <file>nlsltp.c</file>
    <file>nlsltu.c</file>
    <file>nls_endpoints.c</file>
    <file>nls_endpoint_test.c</file>
    <file>nls_endpoint_dump.c</file>
    <file>nls_endpoint_interpret.c</file>
    <file>nls_endpoint_packages.c</file>
    <file>nlsmt.c</file>
  </sources>
  <platforms>
    <platform name="linux">
      <option name="NAME">ognls</option>
      <option name="ADD_CFLAGS"/>
      <option name="ADD_LIBR">-lognlp -logaddr -loguci -logsock -logthr -logheap -logmsg -loggen -lglib-2.0 -ljansson -luriparser</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
  </platforms>
</makefile>
