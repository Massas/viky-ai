<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>prog</type>
  <project_name>gettoklex</project_name>
  <sources>
    <file>gettoklex.c</file>
  </sources>
  <platforms>
    <platform name="win">
      <option name="NAME">gettoklex.exe</option>
      <option name="ADD_LIBR">ogm_thr_a.lib ogm_gen_a.lib ogm_aut_a.lib</option>
      <option name="ADD_LIBD">ogm_thr_a.lib ogm_gen_a.lib ogm_aut_a.lib</option>
      <option name="ADD_LIBP">ogm_thr_a.lib ogm_gen_a.lib ogm_aut_a.lib</option>
    </platform>
    <platform name="linux">
      <option name="NAME">gettoklex</option>
      <option name="ADD_CFLAGS"></option>
      <option name="ADD_LIBR">$(RLINKPATH)/libogaut.a $(RLINKPATH)/liboggen.a $(RLINKPATH)/libogthr.a</option>
      <option name="ADD_LIBD">$(DLINKPATH)/libogaut.a $(DLINKPATH)/liboggen.a $(DLINKPATH)/libogthr.a</option>
      <option name="ADD_LIBP">$(PLINKPATH)/libogaut.a $(PLINKPATH)/liboggen.a $(PLINKPATH)/libogthr.a</option>
    </platform>
  </platforms>
</makefile>
