<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>lib</type>
  <project_name>ogm_nls</project_name>
  <sources>
    <file>inls.c</file>
    <file>nlsconf.c</file>
    <file>nlsonem.c</file>
    <file>nlsrun.c</file>
    <file>nlslog.c</file>
    <file>nlslt.c</file>
    <file>nlslter.c</file>
    <file>nlsltp.c</file>
    <file>nlsltu.c</file>
    <file>nls_json.c</file>
    <file>nls_dataEngine.c</file>
    <file>nlsmt.c</file>
  </sources>
  <platforms>
    <platform name="linux">
      <option name="NAME">ognls</option>
      <option name="ADD_CFLAGS"/>
      <option name="ADD_LIBR">-logaddr -loguci -logsock -logthr -logheap -logmsg -loggen -lglib-2.0 -lyajl -ljansson -lulfius -lyder -lorcania -lmicrohttpd</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
  </platforms>
</makefile>
