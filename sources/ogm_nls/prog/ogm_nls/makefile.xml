<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE makefile SYSTEM "makefile.dtd">
<makefile>
  <type>prog</type>
  <project_name>ogm_nls</project_name>
  <sources>
    <file>ogm_nls.c</file>
  </sources>
  <platforms>
    <platform name="linux">
      <option name="NAME">ogm_nls</option>
      <sources>
        <file>nlsdaem.c</file>
      </sources>
      <option name="ADD_CFLAGS"/>
      <option name="ADD_LIBR">-loggen -logmsg -logsig -lognls -logthr -lglib-2.0</option>
      <option name="ADD_LIBD">$(ADD_LIBR)</option>
      <option name="ADD_LIBP">$(ADD_LIBR)</option>
    </platform>
  </platforms>
</makefile>
