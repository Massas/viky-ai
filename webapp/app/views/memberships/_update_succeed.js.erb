<%
  message = t(
    "views.memberships.update_succeed.#{@membership.rights}",
    user: @membership.user.name_or_username,
    agent: @membership.agent.name
  )
%>
App.Message.notice("<%= escape_javascript(message) %>");
$("#membership-<%= @membership.id %>").html("<%= escape_javascript(@modal_content) %>");
$("body").trigger("Agent:injectOrigin");
$("#agent-<%= @membership.agent.id %>").replaceWith("<%= escape_javascript(@agent_html) %>");
