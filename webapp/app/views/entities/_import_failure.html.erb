<div class="banner banner--alert">
  <div class="banner__content">
    <div>
      <img src='<%= asset_path("robot-confused.png") %>'
           srcset='<%= asset_path("robot-confused.png") %> 1x,
                   <%= asset_path("robot-confused@2x.png") %> 2x'
           style='width: 80px'/>
    </div>
    <div>
      <h3>
        <%= t('views.entities_lists.show.import.failure.title') %>
      </h3>
      <p>
        <%= t('views.entities_lists.show.import.failure.content') %>
      </p>
      <ul>
        <% errors.each do |error| %>
          <li><%= error %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
