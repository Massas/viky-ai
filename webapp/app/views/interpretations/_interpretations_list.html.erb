<ul id="interpretations-list" class="card-list"
    data-behavior="sortable"
    data-update-positions-path="<%= update_positions_user_agent_intent_interpretations_path(@agent.owner, @agent, @intent) %>"
    data-key='ids'
    data-group="interpretations">
  <%= render partial: '/interpretations/interpretation', collection: @intent.interpretations_with_local(@current_locale).order(position: :desc, created_at: :desc) %>
</ul>
